//Largest prime factor
#include <iostream>
using namespace std;

int n;
int s;
int maxim;
int ultima_cifra;
int Y;
int sumaCifre(int n)
{
   while(n)
   {
       s += n%10;
       n /= 10;
   } 
   return s;
}
int prim(int n)
{
    if(n < 2)
        return 0;
    if(n > 2 && n % 2 == 0)
        return 0;
    for(int d = 3; d * d <= n; d += 2)
        if(n % d == 0)
            return 0;
    return 1;
}
int factorPrim(int n)
{
    Y = 2;
    if(prim(n))
        return n;
    else
    {
        if(n % Y == 0)
        {
            maxim = Y;
           return factorPrim(n / Y);
        }
        else
        { 
            Y++;
            return factorPrim(n);
        }
    }
    //return maxim;
}
int main()
{
    cin>>n;
    cout<<factorPrim(n);
    return 0;
}
